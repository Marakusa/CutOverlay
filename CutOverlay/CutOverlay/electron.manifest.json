{
	"executable": "CutOverlay",
	"splashscreen": {
		"imageFile": ""
	},
	"name": "CUTOverlay",
	"author": "Markus Kannisto",
	"singleInstance": false,
	"aspCoreBackendPort": 37100,
	"environment": "Production",
	"build": {
		"appId": "com.CutOverlay.app",
		"productName": "CUT Overlay",
		"copyright": "Copyright Â© Markus Kannisto 2023",
		"buildVersion": "0.1.4-rc3",
		"win": {
			"icon": "./public/icons/ico/favicon.ico"
		},
		"compression": "maximum",
		"directories": {
			"output": "../../../bin/Desktop",
			"buildResources": "public"
		},
		"extraResources": [
			{
				"from": "./bin",
				"to": "bin",
				"filter": ["**/*"]
			}
		],
		"files": [
			{
				"from": "./ElectronHostHook/node_modules",
				"to": "ElectronHostHook/node_modules",
				"filter": ["**/*"]
			},
			"**/*",
			"./public/**/*"
		]
	}
}